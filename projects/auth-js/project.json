{
    "name": "auth-js",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "dist/auth-js"
            ],
            "options": {
                "commands": [
                    "node projects/auth-js/build.mjs"
                ],
                "color": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "projects/auth-js/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/projects/auth-js"
            ],
            "options": {
                "jestConfig": "projects/auth-js/jest.config.js",
                "passWithNoTests": true
            }
        }
    }
}
