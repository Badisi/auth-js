{
    "name": "demo-app-common",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "projects/demo-app/web/common",
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "projects/demo-app/web/common/**/*.ts",
                    "projects/demo-app/web/common/**/*.html"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/projects/demo-app/web/common"
            ],
            "options": {
                "jestConfig": "projects/demo-app/web/common/jest.config.js",
                "passWithNoTests": true
            }
        }
    }
}
