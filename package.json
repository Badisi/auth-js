{
    "name": "@badisi/auth-js",
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0-only",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn",
        "web",
        "mobile"
    ],
    "private": true,
    "scripts": {
        "ncu": "npx npm-check-updates -i --format=group --packageFile='{,projects/**/}package.json'",
        "lint": "nx run-many --target=lint --projects=auth-js,ngx-auth",
        "test": "nx run-many --target=test --projects=auth-js,ngx-auth",
        "publish": "multi-semantic-release --ignore-private-packages --yes",
        "publish-dry-run": "GITHUB_TOKEN=XXX NPM_TOKEN=XXX npm run publish -- --no-ci",
        "postinstall": "node ./scripts/patch-semantic-release-npm.js",
        "prepare": "husky || true"
    },
    "workspaces": [
        "projects/auth-js",
        "projects/demo-app/capacitor",
        "projects/demo-app/web",
        "projects/demo-e2e",
        "projects/demo-idp",
        "projects/ngx-auth",
        "projects/site"
    ],
    "dependencies": {
        "@angular/common": "12.2.17",
        "@angular/compiler": "12.2.17",
        "@angular/core": "12.2.17",
        "@angular/forms": "12.2.17",
        "@angular/platform-browser": "12.2.17",
        "@angular/platform-browser-dynamic": "12.2.17",
        "@angular/router": "12.2.17",
        "rxjs": "^7.8.1",
        "tslib": "^2.6.2",
        "zone.js": "0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "12.2.18",
        "@angular/cli": "12.2.18",
        "@angular/compiler-cli": "12.2.17",
        "@commitlint/cli": "^18.6.1",
        "@commitlint/config-conventional": "^18.6.2",
        "@hug/eslint-config": "^17.0.1",
        "@nrwl/angular": "12.10.1",
        "@nrwl/cli": "^15.9.3",
        "@nrwl/eslint-plugin-nx": "15.8.6",
        "@nrwl/jest": "14.7.3",
        "@nrwl/linter": "15.8.6",
        "@nrwl/tao": "15.8.6",
        "@nrwl/web": "15.8.6",
        "@nrwl/webpack": "15.8.6",
        "@nrwl/workspace": "15.8.6",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.19",
        "cpy-cli": "^5.0.0",
        "esbuild": "^0.20.0",
        "husky": "^9.0.11",
        "jest": "28.1.3",
        "jest-environment-jsdom": "^29.7.0",
        "jest-preset-angular": "12.2.6",
        "lint-staged": "^15.2.2",
        "multi-semantic-release": "^3.0.2",
        "ng-packagr": "12.2.7",
        "nx": "15.8.6",
        "postcss": "^8.4.35",
        "postcss-import": "^16.0.1",
        "postcss-preset-env": "^9.3.0",
        "postcss-url": "^10.1.3",
        "ts-jest": "28.0.8",
        "typescript": "4.3.5"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "**/*.{js,json,ts,html}": [
            "eslint --fix"
        ]
    }
}
