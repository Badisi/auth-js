{
    "name": "@badisi/auth-js",
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0-only",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn",
        "web",
        "mobile"
    ],
    "private": true,
    "type": "module",
    "scripts": {
        "ncu": "npx npm-check-updates -i --format=group --packageFile='{,projects/**/}package.json'",
        "migrate": "nx migrate latest",
        "prettier": "prettier . --check",
        "lint": "nx run-many --target=lint --projects=auth-js,ngx-auth,demo-app-web --output-style=static",
        "test": "nx run-many --target=test --projects=auth-js,ngx-auth --output-style=static",
        "publish": "multi-semantic-release --ignore-private-packages --yes",
        "publish-dry-run": "GITHUB_TOKEN=XXX NPM_TOKEN=XXX npm run publish -- --dry-run --no-ci",
        "prepare": "husky || true"
    },
    "workspaces": [
        "apps/demo-app/capacitor",
        "apps/demo-app/web",
        "apps/demo-e2e",
        "apps/demo-idp",
        "apps/website",
        "libs/auth-js",
        "libs/ngx-auth"
    ],
    "dependencies": {
        "@angular/common": "~18.2.0",
        "@angular/compiler": "~18.2.0",
        "@angular/core": "~18.2.0",
        "@angular/forms": "~18.2.0",
        "@angular/platform-browser": "~18.2.0",
        "@angular/platform-browser-dynamic": "~18.2.0",
        "@angular/router": "~18.2.0",
        "rxjs": "~7.8.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~18.2.0",
        "@angular-devkit/core": "~18.2.0",
        "@angular-devkit/schematics": "~18.2.0",
        "@angular/cli": "~18.2.0",
        "@angular/compiler-cli": "~18.2.0",
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@hug/eslint-config": "^21.0.0-beta.7",
        "@nx/angular": "^20.1.3",
        "@nx/eslint": "20.1.3",
        "@nx/eslint-plugin": "20.1.3",
        "@nx/jest": "20.1.3",
        "@nx/js": "20.1.4",
        "@nx/web": "20.1.4",
        "@nx/webpack": "20.1.4",
        "@nx/workspace": "20.1.3",
        "@types/jest": "^29.5.12",
        "@types/node": "18.16.9",
        "autoprefixer": "^10.4.0",
        "cpy-cli": "^5.0.0",
        "esbuild": "^0.19.2",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-preset-angular": "~14.1.0",
        "lint-staged": "^15.2.10",
        "ng-packagr": "~18.2.0",
        "nx": "20.1.3",
        "postcss": "^8.4.5",
        "postcss-url": "~10.1.3",
        "prettier": "^3.0.0",
        "prettier-plugin-multiline-arrays": "^3.0.6",
        "ts-jest": "^29.1.0",
        "tslib": "^2.3.0",
        "typescript": "~5.5.2"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "**/*.{js,json,ts,html}": [
            "npm run lint"
        ],
        "**/*.{scss,html,json}": [
            "npm run prettier"
        ]
    },
    "packageManager": "npm@10.9.0"
}
