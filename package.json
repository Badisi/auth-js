{
    "name": "@badisi/ngx-auth",
    "version": "1.0.0-beta.3",
    "description": "Authentication and authorization support for angular based client applications.",
    "homepage": "https://github.com/Badisi/ngx-auth",
    "license": "GPL-3.0",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/ngx-auth.git"
    },
    "scripts": {
        "start": "ng serve --host 0.0.0.0",
        "start:idp": "docker-compose --file='./idp/docker-compose.yml' up",
        "build": "ng build ngx-auth-demo --prod",
        "test": "ng test ngx-auth-demo",
        "lint": "ng lint ngx-auth-demo",
        "e2e": "ng e2e ngx-auth-demo",
        "build:lib": "ng build @badisi/ngx-auth --prod && cpy ./projects/ngx-auth/oidc/assets/**/* ./dist/ngx-auth/oidc/assets && downlevel-dts ./dist/ngx-auth ./dist/ngx-auth",
        "test:lib": "ng test @badisi/ngx-auth",
        "lint:lib": "ng lint @badisi/ngx-auth",
        "publish:lib": "cd dist/ngx-auth && npm publish --unsafe-perm"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "~11.0.1",
        "@angular/compiler": "~11.0.1",
        "@angular/core": "~11.0.1",
        "@angular/forms": "~11.0.1",
        "@angular/platform-browser": "~11.0.1",
        "@angular/platform-browser-dynamic": "~11.0.1",
        "@angular/router": "~11.0.1",
        "lodash-es": "^4.17.15",
        "oidc-client": "1.10.1",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.3",
        "@angular/cli": "~11.0.2",
        "@angular/compiler-cli": "~11.0.1",
        "@types/jasmine": "~3.6.0",
        "@types/lodash-es": "^4.17.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "cpy-cli": "^3.1.1",
        "downlevel-dts": "^0.7.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.0.0",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2"
    }
}
