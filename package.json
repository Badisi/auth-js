{
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn"
    ],
    "private": true,
    "scripts": {
        "start:docs": "npm run start --prefix=projects/docs",
        "start:demo-idp": "docker-compose --file='projects/demo-idp/docker-compose.yml' up",
        "start:demo-app": "nx run demo-app:serve --host 0.0.0.0",
        "lint:demo-app": "nx run demo-app:lint",
        "lint:auth-js": "nx run auth-js:lint",
        "lint:ngx-auth": "nx run ngx-auth:lint",
        "lint": "npm run lint:demo-app && npm run lint:auth-js && npm run lint:ngx-auth",
        "build:demo-app": "nx run demo-app:build",
        "build:auth-js": "nx run auth-js:build",
        "build:ngx-auth": "nx run ngx-auth:build && cpy projects/auth-js/oidc/assets dist/ngx-auth/oidc/assets && cpy LICENSE dist/ngx-auth",
        "build": "npm run build:auth-js && npm run build:ngx-auth",
        "e2e": "npm run start --prefix=projects/demo-e2e -- --debug=true --headless=false --watch=true",
        "postinstall": "ngcc --properties es2015 browser module main"
    },
    "dependencies": {
        "@angular/common": "^12.x",
        "@angular/compiler": "^12.x",
        "@angular/core": "^12.x",
        "@angular/forms": "^12.x",
        "@angular/platform-browser": "^12.x",
        "@angular/platform-browser-dynamic": "^12.x",
        "@angular/router": "^12.x",
        "jwt-decode": "^3.1.2",
        "lodash-es": "^4.17.21",
        "oidc-client-ts": "2.0.0",
        "rxjs": "^7.5.3",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.x",
        "@angular/cli": "^12.x",
        "@angular/compiler-cli": "^12.x",
        "@hug/eslint-config": "^5.1.0",
        "@nrwl/angular": "^12.x",
        "@nrwl/cli": "^13.8.0",
        "@nrwl/eslint-plugin-nx": "^13.8.0",
        "@nrwl/jest": "^13.8.0",
        "@nrwl/js": "^13.8.0",
        "@nrwl/linter": "^13.8.0",
        "@nrwl/tao": "^13.8.0",
        "@nrwl/workspace": "^13.8.0",
        "@types/jest": "^27.4.0",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^17.0.16",
        "@wdio/cli": "^7.16.14",
        "@wdio/jasmine-framework": "^7.16.14",
        "@wdio/local-runner": "^7.16.14",
        "@wdio/spec-reporter": "^7.16.14",
        "chromedriver": "^97.0.4",
        "cpy-cli": "^3.1.1",
        "eslint": "^8.8.0",
        "jest": "^27.5.1",
        "jest-preset-angular": "^11.1.0",
        "ng-packagr": "^12.x",
        "postcss": "^8.4.6",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^7.3.1",
        "postcss-url": "^10.1.1",
        "ts-jest": "^27.1.3",
        "typescript": "~4.2.3",
        "wdio-chromedriver-service": "^7.2.6",
        "wdio-intercept-service": "^4.2.1"
    }
}
