{
    "name": "@badisi/auth-js",
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0-only",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn",
        "web",
        "mobile"
    ],
    "private": true,
    "scripts": {
        "start:site": "npm run start --prefix=projects/site",
        "start:demo-idp": "npm run start --prefix=projects/demo-idp",
        "start:demo-app:auth-js": "nx run demo-app-auth-js:serve",
        "start:demo-app:ngx-auth": "nx run demo-app-ngx-auth:serve",
        "lint:auth-js": "nx run auth-js:lint",
        "lint:ngx-auth": "nx run ngx-auth:lint",
        "lint:demo-app": "nx run demo-app:lint",
        "lint": "nx run-many --target=lint --projects=auth-js,ngx-auth,demo-app",
        "test": "nx run ngx-auth:test",
        "build:auth-js": "nx run auth-js:build",
        "build:demo-app-auth-js": "nx run demo-app-auth-js:build",
        "build:ngx-auth": "nx run ngx-auth:build && nx run ngx-auth:copy-assets && nx run ngx-auth:build-schematics",
        "build:demo-app-ngx-auth": "nx run demo-app-ngx-auth:build",
        "e2e": "npm run start --prefix=projects/demo-e2e -- --debug=true --headless=false --watch=true",
        "postinstall": "ngcc --properties es2015 browser module main"
    },
    "workspaces": [
        "projects/auth-js",
        "projects/ngx-auth",
        "projects/demo-e2e"
    ],
    "dependencies": {
        "@angular/common": "^12.x",
        "@angular/compiler": "^12.x",
        "@angular/core": "^12.x",
        "@angular/forms": "^12.x",
        "@angular/platform-browser": "^12.x",
        "@angular/platform-browser-dynamic": "^12.x",
        "@angular/router": "^12.x",
        "rxjs": "^7.5.5",
        "tslib": "^2.4.0",
        "zone.js": "^0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.x",
        "@angular/cli": "^12.x",
        "@angular/compiler-cli": "^12.x",
        "@hug/eslint-config": "^7.0.2",
        "@nrwl/angular": "^12.x",
        "@nrwl/cli": "^14.1.9",
        "@nrwl/eslint-plugin-nx": "^14.1.9",
        "@nrwl/jest": "^14.1.9",
        "@nrwl/linter": "^14.1.9",
        "@nrwl/tao": "^14.1.9",
        "@nrwl/web": "^14.1.9",
        "@nrwl/workspace": "^14.1.9",
        "@types/jest": "^28.1.0",
        "@types/node": "^17.0.39",
        "cpy-cli": "^4.1.0",
        "esbuild": "^0.14.42",
        "eslint": "^8.17.0",
        "jest": "^28.1.0",
        "jest-preset-angular": "^12.1.0",
        "ng-packagr": "^12.x",
        "postcss": "^8.4.14",
        "postcss-import": "^14.1.0",
        "postcss-preset-env": "^7.7.1",
        "postcss-url": "^10.1.1",
        "ts-jest": "^28.0.4",
        "typescript": "~4.3.5"
    }
}
