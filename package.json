{
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn",
        "web",
        "mobile"
    ],
    "private": true,
    "scripts": {
        "start:site": "npm run start --prefix=projects/site",
        "start:demo-idp": "npm run start --prefix=project/demo-idp",
        "start:demo-app:auth-js": "nx run demo-app-auth-js:serve",
        "start:demo-app:ngx-auth": "nx run demo-app-ngx-auth:serve",
        "lint:auth-js": "nx run auth-js:lint",
        "lint:ngx-auth": "nx run ngx-auth:lint",
        "lint:demo-app": "nx run demo-app:lint",
        "lint": "nx run-many --target=lint --projects=auth-js,ngx-auth,demo-app",
        "build:auth-js": "nx run auth-js:build",
        "build:demo-app-auth-js": "nx run demo-app-auth-js:build",
        "build:ngx-auth": "nx run ngx-auth:build && nx run ngx-auth:copy-assets",
        "build:demo-app-ngx-auth": "nx run demo-app-ngx-auth:build",
        "e2e": "npm run start --prefix=projects/demo-e2e -- --debug=true --headless=false --watch=true",
        "postinstall": "ngcc --properties es2015 browser module main"
    },
    "dependencies": {
        "@angular/common": "^12.x",
        "@angular/compiler": "^12.x",
        "@angular/core": "^12.x",
        "@angular/forms": "^12.x",
        "@angular/platform-browser": "^12.x",
        "@angular/platform-browser-dynamic": "^12.x",
        "@angular/router": "^12.x",
        "jwt-decode": "^3.1.2",
        "lodash-es": "^4.17.21",
        "oidc-client-ts": "^2.0.1",
        "rxjs": "^7.5.5",
        "tslib": "^2.0.0",
        "zone.js": "^0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^12.x",
        "@angular/cli": "^12.x",
        "@angular/compiler-cli": "^12.x",
        "@hug/eslint-config": "^6.0.0",
        "@nrwl/angular": "^12.x",
        "@nrwl/cli": "^13.9.1",
        "@nrwl/eslint-plugin-nx": "^13.9.1",
        "@nrwl/jest": "^13.9.1",
        "@nrwl/linter": "^13.9.1",
        "@nrwl/tao": "^13.9.1",
        "@nrwl/web": "^13.9.1",
        "@nrwl/workspace": "^13.9.1",
        "@types/jest": "^27.4.1",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^17.0.21",
        "@wdio/cli": "^7.17.4",
        "@wdio/jasmine-framework": "^7.17.3",
        "@wdio/local-runner": "^7.17.4",
        "@wdio/spec-reporter": "^7.17.3",
        "chromedriver": "^99.0.0",
        "cpy-cli": "^4.1.0",
        "esbuild": "^0.14.27",
        "eslint": "^8.11.0",
        "jest": "^27.5.1",
        "jest-preset-angular": "^11.1.1",
        "ng-packagr": "^12.x",
        "postcss": "^8.4.12",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^7.4.1",
        "postcss-url": "^10.1.1",
        "ts-jest": "^27.1.3",
        "typescript": "~4.3.5",
        "wdio-chromedriver-service": "^7.3.1",
        "wdio-intercept-service": "^4.2.2"
    }
}
