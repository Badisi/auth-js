{
    "name": "@badisi/auth-js",
    "description": "Authentication and authorization support for web based desktop and mobile applications.",
    "homepage": "https://github.com/Badisi/auth-js",
    "license": "GPL-3.0-only",
    "author": "Badisi",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Badisi/auth-js.git"
    },
    "keywords": [
        "authentication",
        "authorization",
        "oidc",
        "openidconnect",
        "openid",
        "security",
        "identity",
        "oauth2",
        "oauth",
        "auth",
        "authn",
        "web",
        "mobile"
    ],
    "private": true,
    "type": "module",
    "scripts": {
        "ncu": "npx npm-check-updates -i --format=group --packageFile='{,apps/**/,libs/**/}package.json'",
        "migrate": "nx migrate latest",
        "migrate:cap": "cap migrate",
        "prettier": "prettier . --check",
        "lint": "eslint .",
        "test": "nx run-many --target=test --projects=auth-js,ngx-auth --output-style=static",
        "publish": "multi-semantic-release --ignore-private-packages --yes",
        "publish-dry-run": "GITHUB_TOKEN=XXX NPM_TOKEN=XXX npm run publish -- --dry-run --no-ci",
        "prepare": "husky || true"
    },
    "workspaces": [
        "apps/demo-app/capacitor",
        "apps/demo-app/web",
        "apps/demo-e2e",
        "apps/demo-idp",
        "apps/website",
        "libs/auth-js",
        "libs/auth-vue",
        "libs/ngx-auth"
    ],
    "dependencies": {
        "@angular/common": "~18.2.0",
        "@angular/compiler": "~18.2.0",
        "@angular/core": "~18.2.0",
        "@angular/forms": "~18.2.0",
        "@angular/platform-browser": "~18.2.0",
        "@angular/platform-browser-dynamic": "~18.2.0",
        "@angular/router": "~18.2.0",
        "rxjs": "~7.8.1",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0",
        "zone.js": "~0.14.10"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~18.2.0",
        "@angular-devkit/core": "~18.2.0",
        "@angular-devkit/schematics": "~18.2.0",
        "@angular/cli": "~18.2.0",
        "@angular/compiler-cli": "~18.2.0",
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@hug/eslint-config": "^21.0.0-beta.7",
        "@nx/angular": "^20.3.0",
        "@nx/eslint": "20.3.0",
        "@nx/eslint-plugin": "20.3.0",
        "@nx/jest": "20.3.0",
        "@nx/js": "20.3.0",
        "@nx/vite": "20.3.0",
        "@nx/vue": "20.3.0",
        "@nx/web": "20.3.0",
        "@nx/webpack": "20.3.0",
        "@nx/workspace": "20.3.0",
        "@types/jest": "^29.5.14",
        "@types/node": "22.10.5",
        "@vitejs/plugin-vue": "^5.2.1",
        "autoprefixer": "^10.4.20",
        "cpy-cli": "^5.0.0",
        "detect-indent": "^7.0.1",
        "esbuild": "^0.24.2",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-preset-angular": "~14.4.2",
        "lint-staged": "^15.3.0",
        "ng-packagr": "~18.2.0",
        "nx": "20.3.0",
        "patch-package": "^8.0.0",
        "postcss": "^8.4.49",
        "postcss-url": "~10.1.3",
        "prettier": "^3.4.2",
        "prettier-plugin-multiline-arrays": "^4.0.2",
        "ts-jest": "^29.2.5",
        "tslib": "^2.8.1",
        "typescript": "~5.5.4",
        "vite": "^5.0.0",
        "vite-plugin-static-copy": "^2.2.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "lint-staged": {
        "**/*.{js,json,ts,html}": [
            "eslint"
        ],
        "**/*.{scss,html,vue,json}": [
            "prettier --check"
        ]
    },
    "packageManager": "npm@11.0.0"
}
